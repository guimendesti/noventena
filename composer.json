{
    "name": "noventena",
    "description":"API para Regra Noventena (NPS)",
    "require": {
        "php": ">=7.1",
        "illuminate/database": "~5.1",
        "monolog/monolog": "^1.17",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.12",
        "slim/http-cache": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\" : "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
